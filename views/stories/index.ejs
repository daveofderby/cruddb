<%- include('../partials/header')%>
<div class="container">
  <h1>Storybook List</h1>

  <table class="table table-striped table-dark table-hover">
    <tr>
      <th>Title</th>
      <th>Category</th>
      <th>Lat</th>
      <th>Long</th>
    </tr>

    <% for (let story of storyData) {%>
    <tr onclick="javascript:window.location='/stories/<%=story.id%>';">
      <td><%= story.title %></td>
      <td><%= story.category %></td>
      <td><%= story.lat %></td>
      <td><%= story.long %></td>
    </tr>
    <% } %>
  </table>
  <a href="/stories/new"
    ><button type="button" class="btn btn-primary">Add New Story</button></a
  >
</div>
<%- include('../partials/footer')%>
